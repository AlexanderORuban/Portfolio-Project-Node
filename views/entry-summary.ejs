<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="styles/entry-summary.css">  
    <title>Admin</title>
</head>

<body>
    <div class="container">
        <h1>Entry Summary</h1>
        <table border="1">
            <thead>
                <tr>
                    <th>ID</th>
                    <th>First Name</th>
                    <th>Last Name</th>
                    <th>Job Title</th>
                    <th>Company</th>
                    <th>LinkedIn URL</th>
                    <th>Email</th>
                    <th>How we met</th>
                    <th>Message</th>
                    <th>Date submitted</th>
                </tr>
            </thead>
            <tbody>
                <% entries.forEach(data => { %>
                    <tr>
                        <td><%= data.id %></td>
                        <td><%= data.fname %></td>
                        <td><%= data.lname %></td>
                        <td><%= data.job %></td>
                        <td><%= data.company %></td>
                        <td><%= data.linkedin %></td>
                        <td><%= data.email %></td>
                        <td><%= data.met %></td>
                        <td><%= data.message %></td>
                        <td><%= data.date_submitted %></td>
                    </tr>
                <% }); %>
            </tbody>
        </table>
    </div>
</body>
</html>